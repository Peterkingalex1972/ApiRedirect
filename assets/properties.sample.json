{
  "default": "google",
  "google": {
    "host": "https://google.com",
    "port": 4000,
    "timeout": 15000,
    "root_folder": "/path/to/project",
    "bind": [
      {
        "uri": "/index",
        "path": "/index.html"
      },
      {
        "uri": "/resources/javascripts/",
        "path": "/resources/javascripts/"
      },
      {
        "uri": "/resources/stylesheets/",
        "path": "/resources/stylesheets/"
      },
      {
        "uri": "/resources/images/",
        "path": "/resources/images/"
      },
      {
        "uri": "/resources/fonts/",
        "path": "/resources/fonts/"
      }
    ],
    "headers": {
      "Authorization": "Basic TsOjbyBow6EgbmFkYSBwb3IgYXF1aSA7KQ=="
    },
    "override_responses": {
      "/login": [
        {
          "field": "targetUrl",
          "value": "http://localhost:3000/index"
        }
      ]
    }
  },
  "log": {
    "colors": true,
    "print_response": false,
    "print_request_url": true,
    "print_request_error": false
  },
  "facebook": {
    "host": "https://facebook.com",
    "port": 3000,
    "root_folder": "/path/to/project",
    "bind": [
      {
        "uri": "/index",
        "path": "/index.html"
      },
      {
        "uri": "/resources/",
        "path": "/resources/"
      }
    ]
  }
}