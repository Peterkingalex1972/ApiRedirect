{
  "default": "google",
  "google": {
    "host": "https://google.com",
    "port": 4000,
    "timeout": 15000,
    "root_folder": "/path/to/project",
    "bind": [
      {
        "uri": "/index",
        "path": "/index.html"
      },
      {
        "uri": "/resources/javascripts/",
        "path": "/resources/javascripts/"
      },
      {
        "uri": "/resources/stylesheets/",
        "path": "/resources/stylesheets/"
      },
      {
        "uri": "/resources/images/",
        "path": "/resources/images/"
      },
      {
        "uri": "/resources/fonts/",
        "path": "/resources/fonts/"
      }
    ],
    "headers": {
      "Authorization": "Basic dsOhIHNlIGZ1ZGVyIHNldSBvdMOhcmlvIHBhdSBubyBjw7o="
    },
    "override_responses": [
      {
        "uri": "/login",
        "field": "target_url",
        "value": "http://localhost:3000"
      }
    ]
  },
  "facebook": {
    "host": "https://facebook.com",
    "port": 3000,
    "root_folder": "/path/to/project",
    "bind": [
      {
        "uri": "/index",
        "path": "/index.html"
      },
      {
        "uri": "/resources/",
        "path": "/resources/"
      }
    ],
    "override_responses": [
      {
        "uri": "/test",
        "field": "target.test",
        "value": 4
      },
      {
        "uri": "/test2",
        "field": "target.test2",
        "value": "simple_value"
      }
    ]
  }
}