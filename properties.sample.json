{
  "default": "API_ALIAS",
  "API_ALIAS": {
    "api": {
      "host": "https://google.com",
      "port": 4000
    },
    "root_folder": "/path/project",
    "bind": [
      {
        "path": "/index",
        "uri": "/index.html"
      },
      {
        "path": "/resources/javascripts/",
        "uri": "/resources/javascripts/"
      },
      {
        "path": "/resources/stylesheets/",
        "uri": "/resources/stylesheets/"
      },
      {
        "path": "/resources/images/",
        "uri": "/resources/images/"
      },
      {
        "path": "/resources/fonts/",
        "uri": "/resources/fonts/"
      }
    ]
  },
  "API_ALIAS_2": {
    "api": {
      "host": "https://facebook.com",
      "port": 3000
    },
    "root_folder": "/path/project",
    "bind": [
      {
        "path": "/index",
        "uri": "/index.html"
      },
      {
        "path": "/resources/stylesheets/",
        "uri": "/resources/stylesheets/"
      }
    ]
  }
}
