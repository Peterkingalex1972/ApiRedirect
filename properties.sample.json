{
  "default": "google",
  "google": {
    "api": {
      "host": "https://google.com",
      "port": 4000
    },
    "root_folder": "/path/to/project",
    "bind": [
      {
        "uri": "/index",
        "path": "/index.html"
      },
      {
        "uri": "/resources/javascripts/",
        "path": "/resources/javascripts/"
      },
      {
        "uri": "/resources/stylesheets/",
        "path": "/resources/stylesheets/"
      },
      {
        "uri": "/resources/images/",
        "path": "/resources/images/"
      },
      {
        "uri": "/resources/fonts/",
        "path": "/resources/fonts/"
      }
    ],
    "override_responses": [
      {
        "uri": "/login",
        "field": "target_url",
        "value": "http://localhost:3000"
      }
    ]
  },
  "facebook": {
    "api": {
      "host": "https://facebook.com",
      "port": 3000
    },
    "root_folder": "/path/to/project",
    "bind": [
      {
        "uri": "/index",
        "path": "/index.html"
      },
      {
        "uri": "/resources/stylesheets/",
        "path": "/resources/stylesheets/"
      }
    ],
    "override_responses": [
      {
        "uri": "/test",
        "field": "target.test",
        "value": 4
      },
      {
        "uri": "/test2",
        "field": "target.test2",
        "value": "simple_value"
      }
    ]
  }
}